{
	"manifest_version": 2,
	"name": "Vino AutoLike",
	"description": "Лайк за лайк",
	"version": "1.0.0",
	"icons": {
		"128": "img/vivino-icon.png"
	},
	"content_scripts": [
		{
			"matches": [
				"*://*/*"
			],
			"js": [
				"js/content.js"
			]
		}
	],
	"background": {
		"scripts": [
			"js/background.js"
		],
		"persistent": false
	},
	"permissions": [
		"activeTab"
	]
}